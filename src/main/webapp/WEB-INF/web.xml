<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd"
  version="2.4">

  <display-name>IIS Kernel</display-name>
  
  <welcome-file-list>  
  	<welcome-file>home</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>pop</servlet-name>
    <servlet-class>org.immregistries.iis.kernal.servlet.PopServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>home</servlet-name>
    <servlet-class>org.immregistries.iis.kernal.servlet.HomeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>patient</servlet-name>
    <servlet-class>org.immregistries.iis.kernal.servlet.PatientServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>vaccination</servlet-name>
    <servlet-class>org.immregistries.iis.kernal.servlet.VaccinationServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>fits</servlet-name>
    <servlet-class>org.immregistries.iis.kernal.servlet.FitsServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>lab</servlet-name>
    <servlet-class>org.immregistries.iis.kernal.servlet.LabServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>message</servlet-name>
    <servlet-class>org.immregistries.iis.kernal.servlet.MessageServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>soap</servlet-name>
    <servlet-class>org.immregistries.iis.kernal.servlet.SoapServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>vacDedup</servlet-name>
    <servlet-class>org.immregistries.iis.kernal.servlet.VacDedupServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>home</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>lab</servlet-name>
    <url-pattern>/lab</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>message</servlet-name>
    <url-pattern>/message</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>pop</servlet-name>
    <url-pattern>/pop</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>soap</servlet-name>
    <url-pattern>/soap</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>fits</servlet-name>
    <url-pattern>/fits</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>vacDedup</servlet-name>
    <url-pattern>/vacDedup</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>patient</servlet-name>
    <url-pattern>/patient</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>vaccination</servlet-name>
    <url-pattern>/vaccination</url-pattern>
  </servlet-mapping>


</web-app>
